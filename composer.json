        "documentationUrl": "https://github.com/HOMMinteractive/pixxio/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/HOMMinteractive/pixxio/master/CHANGELOG.md",
        "class": "homm\\hommpixxio\\HOMMPixxio"
