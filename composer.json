{
    "name": "homm/hommpixxio",
    "description": "Craft CMS pixx.io adapter",
    "type": "craft-plugin",
    "version": "0.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "hommpixxio",
        "pixx.io adapter"
    ],
    "support": {
        "docs": "https://github.com/HOMMinteractive/pixxio/blob/master/README.md",
        "issues": "https://github.com/HOMMinteractive/pixxio/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Ammann",
            "homepage": "https://github.com/ammannbe"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
